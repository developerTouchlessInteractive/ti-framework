{
  "name": "ti-framework",
  "version": "0.1.3",
  "description": "sdk for client implementing TouchlessInteractive framework",
  "author": "sailesh n vinay",
  "main": "dist/ti.js",
  "types": "dist/ti.d.ts",
  "scripts": {
    "// ------ DEVELOPMENT ----- //": "",
    "debug": "tsc && nodemon -r esm dist/test-ti.js",
    "debugTest": "tsc && nodemon -r esm jest dist/ti.spec.js",
    "dbgjest": "jest -w",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "// ------ TESTING ----- //": "",
    "test": "jest",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "// ------ BUILDING ----- //": "",
    "build": "tsc",
    "build:watch": "tsc -w",
    "// ------ LINTING ----- //": "",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "// ------ PRODUCTION ----- //": "",
    "start:prod": "nodemon src/ti.ts",
    "push": "tsc && npm publish",
    "vinu": "tsc && nodemon -r esm src/test_client.js",
    "vinu-ti": "tsc && nodemon -r esm src/touchlessinteractive-client-test.js",
    "vinu-ts": "tsc && nodemon -r esm dist/test-ti.js"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/express": "^4.17.7",
    "@types/got": "^9.6.11",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.10.5",
    "@types/socket.io": "^2.1.10",
    "@types/socket.io-client": "^1.4.33",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "got": "^7.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tsyringe": "^4.4.0",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.3.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}